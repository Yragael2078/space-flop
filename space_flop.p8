-- particuls --

function init_particules()
 parts = {}
end

function _make_particule()
 local part = {}
 part.x = 0 -- x position
 part.y = 0 -- y position
 part.sx = 0 -- x speed
 part.sy = 0 -- y speed
 part.ax = 1 -- x acceleration
 part.ay = 1 -- y acceleration
 part.gx = 0 -- x gravity
 part.gy = 0 -- y gravity
 part.col = 7 -- color
 part.life = -1 -- life in frame, infinite if <0
 return part
end

function make_spark(x,y,sx,sy,life,col)
  local p = _make_particule()
  p.x=x or 0
  p.y=y or 0
  p.sx=sx or 0
  p.sy=sy or 0
  p.life=life or -1
  p.col=col or 7
  p.type="spark"
  return p
end

function make_shockwave(x,y,sr,life)
 local p = _make_particule()
 p.r=0
 p.sr=1
 p.type="shockwave"
 return p
end

function update_particules()
 for p in all(parts) do
  if p.life > 0 then
   p.life-=1
  end
  p.x+=p.sx
  p.y+=p.sy
  p.sx=p.sx*p.ax
  p.sy=p.sy*p.ay
  p.sx+=p.gx
  p.sy+=p.gy
  if p.type=="shockwave" then
   p.r+=p.sr
  end
  if p.life==0 or p.x<0 or p.x>127 or p.y<0 or p.y>127 then
   del(parts,p)
  end
 end
end

function draw_particules()
 for p in all(parts) do
  if p.type=="spark" then
   pset(p.x, p.y, p.col)
  elseif p.type=="shockwave" then
   circ(p.x, p.y, p.r, p.col)
  end
 end
end

