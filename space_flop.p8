-- saucer --

function init_saucer()
 saucer={
  img=1,
  x=-8,
  y=60,
  sx=0,
  sy=0,
  g=gravity,
  b=0
 }
end

function update_saucer()
 local sy=saucer.y
 if state=="game" then
  if btnp(❎) then
   saucer.b=boost
   saucer.g=gravity
   sfx(0)
   for i=1,20 do
    local p=make_spark(saucer.x+1+rnd(6),saucer.y+8,-speed,rnd(3)+0.5,5,1)
    local p2=make_spark(saucer.x+2+rnd(4),saucer.y+8,-speed,rnd(2)+0.5,5,12)
    add(parts,p)
    add(parts,p2)
   end
  else
   if saucer.b>0 then
    saucer.b=saucer.b*0.6
   end
   saucer.g=saucer.g*1.1
   saucer.y+=saucer.g-saucer.b
  end
 else
  saucer.x+=saucer.sx
  saucer.y+=saucer.sy
 end
end

function draw_saucer()
 spr(saucer.img,saucer.x,saucer.y)
end

function touch()
 local t=0 -- no touch
 if saucer.y<1 then
  saucer.y=1
  saucer.g=gravity
  saucer.b=0
  t=1
 elseif saucer.y+8>126 then
  saucer.y=126-8
  saucer.g=gravity
  saucer.b=0
  t=2
 else
  for p in all(posts) do
   if saucer.x+8>=p.xpos and saucer.y+8>=p.td and saucer.y+8<p.cd and saucer.x<=p.xpos+16 then
    t=2 --bottom
   end
   if saucer.x+8>=p.xpos and saucer.y<=p.tu and saucer.y>p.cu and saucer.x<=p.xpos+16 then
    t=1 --top
   end
  end
 end
 return t
end

function collide()
 local c=false
 for p in all(posts) do
  if saucer.x+8>=p.xpos and saucer.y+8>=p.cd and saucer.x<=p.xpos+16 then
   c=true
  end
  if saucer.x+8>=p.xpos and saucer.y<=p.cu and saucer.x<=p.xpos+16 then
   c=true
  end
 end
 return c
end

