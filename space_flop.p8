-- logic --

function check_pass()
 if #posts>0 then
  local p=posts[1]
  if pass==false and saucer.x>p.xpos+16 then
   pass=true
   score+=1
   add(posts, make_post())
  end
  if pass==true and saucer.x<p.xpos then
   pass=false
  end
 end
end

function draw_score()
 printl(""..score,127,0,7)
end

function draw_shields()
 if shieldtop==3 then
  spr(4,0,0)
 elseif shieldtop==2 then
  spr(5,0,0)
 end
 if shieldbtm==3 then
  spr(20,0,0)
 elseif shieldbtm==2 then
  spr(21,0,0)
 end
end

function printc(txt,x,y,col)
 print(txt,x-#txt*2,y,col)
end

function printl(txt,x,y,col)
 print(txt,x-#txt*4+2,y,col)
end

