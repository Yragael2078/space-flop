__lua__
-- space flop
-- by yragael2078

 version="1.0"

function _init()
 gravity=0.8 -- fall down acceleration
 boost=12 -- up force
 speed=2 -- general speed
 xpos=25 -- initial horizontal ship position
 gap_size=30 -- size of post gap
 star_density=0.002 -- density of stars on background
 blockout=20 -- number of frames blocking input

 t=0
 tref=0
 cartdata("yragael2078_space-flop_v1")
 highscore=dget(0)
 init_particules()
 init_stars()
 init_game()
end

function _update()
 t+=1
 if state=="start" then
  update_saucer()
  saucer.x+=speed
  if saucer.x>127 then
   saucer.x=-8
  end
  if t-blockout<=0 then
   return
  end
  if btnp(❎) then
   state="flyout"
  end
 elseif state=="flyout" then
  update_saucer()
  saucer.x+=saucer.x*0.1
  if saucer.x>127 then
   state="flyin"
   sfx(3)
   init_saucer()
   saucer.x=-8
   tref=t
  end
 elseif state=="flyin" then
  spd=((xpos+8)-saucer.x)/15
  local brake=true
  if spd<1 then
   spd=1
   brake=false
  end
  if brake==false then
   spd+=spd*0.1
   if spd>=speed then
    spd=speed
   end
  end
  saucer.x+=spd
  if saucer.x>=xpos then
   saucer.x=xpos
   add(posts, make_post())
   state="game"
  end
 elseif state=="game" then
  update_stars()
  update_posts()
  update_saucer()
  update_particules(parts)
  update_particules(pickups)
  bing=touch()
  check_pass()
  if bing==1 then
   saucer.b=-0.2
   --saucer.y=posts[1].tu+2
   saucer.y=saucer.y+2
  elseif bing==2 then
   saucer.b=12
   --saucer.y=posts[1].td-10 -- -8 -2
   saucer.y=saucer.y-2 -- -8 -2
  end
  boom=collide()
  collide_part(pickups)
 elseif state=="gameover" then
  update_particules(parts)
  if t-blockout<=0 then
   return
  end
  if btnp(❎) then
   init_game()
  end
 end
end

function _draw()
 cls()
 draw_stars()
 if state=="start" then
  spr(64,6,50,15,2)
  printc("high score: "..highscore,64,80,7)
  if blockout-t<=0 then
   printc("press ❎ to fly",64,100,6)
  end
  printl("BY yragael2078",126,63,13)
  print("v"..version,1,1,1)
  draw_saucer()
 elseif state=="flyout" then
  spr(64,6,50,15,2)
  printc("high score: "..highscore,64,80,7)
  printl("BY yragael2078",126,63,13)
  print("v"..version,1,1,1)
  draw_saucer()
 elseif state=="flyin" then
  draw_saucer()
  draw_score()
 elseif state=="game" then
  draw_posts()
  draw_saucer()
  draw_score()
  -- print("t:"..shieldtop,70,0,7)
  -- print("b:"..shieldbtm,70,8,7)
  draw_shields()
  if bing==1 then
   shieldtop-=1
   for i=1,5 do
    local s=make_spark(saucer.x+4,saucer.y+2,(rnd()-1.1)*speed,rnd()-1,20)
    add(parts,s)
    sfx(2)
   end
  elseif bing==2 then
   shieldbtm-=1
   for i=1,5 do
    local s=make_spark(saucer.x+4,saucer.y+6,(rnd()-1.1)*speed,rnd(),20)
    add(parts,s)
    sfx(2)
   end
  end
  if shieldtop<=0 or shieldbtm<=0 then
   boom=true
  end
  if boom then
   t=0
   state="gameover"
   sfx(1)
   save_score()
   circfill(saucer.x+4, saucer.y+4, 15, 7)
   for i=1,30 do
    local p=make_spark(
     saucer.x+4,
     saucer.y+4,
     (rnd()-0.3)*9,
     (rnd()-0.5)*9,
     40
    )
    p.ax=0.9
    p.ay=0.9
    p.gx=0.1
    p.gy=0.1
    add(parts,p)
   end
   local sw=make_shockwave()
   sw.x=saucer.x
   sw.y=saucer.y
   sw.sr=5
   sw.life=5
   add(parts,sw)
  end
  draw_particules(parts)
  draw_particules(pickups)
 elseif state=="gameover" then
  draw_posts()
  draw_particules(parts)
  draw_particules(pickups)
  rectfill(43,49,79,55,2)
  print("game over",44,50,7)
  -- print("your score: "..score, 30, 60, 7)
  if highscoreflag then
   printc("new high score!",64,70,7)
  end
  draw_score()
  if blockout-t<=0 then
   rectfill(21,99,101,105,0)
   print("press ❎ to continue",22,100,6)
  end
 end
end

function init_game()
 t=0
 state="start" -- start, game, gameover
 pass=false
 boom=false
 shieldtop=3
 shieldbtm=3
 bing=0
 score=0
 highscoreflag=false
 
 init_posts()
 init_saucer()
 pickups={}
 saucer.x=-8
 saucer.y=40
 saucer.g=0
end

