-- space flop
-- by yragael2078

function _init()
 gravity=0.8 -- fall down acceleration
 boost=12 -- up force
 speed=2 -- general speed
 xpos=25 -- initial horizontal ship position
 gap_size=30 -- size of post gap
 star_density=0.002 -- density of stars on background
 blockout=30 -- number of frames blocking input

 t=0

 init_particules()
 init_stars()
 init_game()
end

function _update()
 t+=1
 if state=="start" then
  if t-blockout<=0 then
   return
  end
  if btnp(❎) then
   state="game"
   add(posts, make_post())
  end
 elseif state=="game" then
  update_stars()
  update_posts()
  update_saucer()
  update_particules()
  constrain_saucer()
  bing=touch()
  check_pass()
  if bing==1 then
   saucer.b=-0.2
   saucer.y=posts[1].tu+2
  elseif bing==2 then
   saucer.b=12
   saucer.y=posts[1].td-10 -- -8 -2
  end
  boom=collide()
 elseif state=="gameover" then
  update_particules()
  if t-blockout<=0 then
   return
  end
  if btnp(❎) then
   init_game()
  end
 end
end

function _draw()
 cls()
 draw_stars()
 if state=="start" then
  printc("space flop",64,50,7)
  if blockout-t<=0 then
   printc("press ❎ to fly",64,70,7)
  end
  printl("yragael2078",127,123,7)
 elseif state=="game" then
  draw_posts()
  draw_saucer()
  draw_score()
  -- print("t:"..shieldtop,70,0,7)
  -- print("b:"..shieldbtm,70,8,7)
  draw_shields()
  if bing==1 then
   shieldtop-=1
   for i=1,5 do
    local s=make_spark(saucer.x+4,saucer.y+2,(rnd()-1.1)*speed,rnd()-1,20)
    add(parts,s)
    sfx(2)
   end
  elseif bing==2 then
   shieldbtm-=1
   for i=1,5 do
    local s=make_spark(saucer.x+4,saucer.y+6,(rnd()-1.1)*speed,rnd(),20)
    add(parts,s)
    sfx(2)
   end
  end
  if shieldtop<=0 or shieldbtm<=0 then
   boom=true
  end
  if boom then
   t=0
   state="gameover"
   sfx(1)
   circfill(saucer.x+4, saucer.y+4, 15, 7)
   for i=1,30 do
    local p=make_spark(
     saucer.x+4,
     saucer.y+4,
     (rnd()-0.3)*9,
     (rnd()-0.5)*9,
     40
    )
    p.ax=0.9
    p.ay=0.9
    p.gx=0.1
    p.gy=0.1
    add(parts,p)
   end
   local sw=make_shockwave()
   sw.x=saucer.x
   sw.y=saucer.y
   sw.sr=5
   sw.life=5
   add(parts,sw)
  end
  draw_particules()
 elseif state=="gameover" then
  draw_posts()
  draw_particules()
  rectfill(43,49,79,55,2)
  print("game over",44,50,7)
  -- print("your score: "..score, 30, 60, 7)
  draw_score()
  if blockout-t<=0 then
   rectfill(21,69,101,75,0)
   print("press ❎ to continue",22,70,7)
  end
 end
end

function init_game()
 t=0
 state="start" -- start, game, gameover
 pass=false
 boom=false
 shieldtop=3
 shieldbtm=3
 bing=0
 score=0
 
 init_posts()
 init_saucer()
end

