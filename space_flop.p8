-- logic --

function check_pass()
 if #posts>0 then
  local p=posts[1]
  if pass==false and saucer.x>p.xpos+16 then
   pass=true
   score+=1
   add(posts, make_post())
  end
  if pass==true and saucer.x<p.xpos then
   pass=false
  end
 end
end

function draw_score()
 print(score,0,0,7)
end

function draw_pause()
 rectfill(33,70,96,80,10)
 print("pause",55,73,0)
 print("press 🅾️",50,83,10)
end

